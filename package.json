{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:element-plus": "turbo run dev --filter=@yza-fe/element-plus",
    "story": "turbo run storybook --filter=storybook",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test:service": "turbo run test --continue --concurrency=1"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.5.12",
    "@storybook/addon-essentials": "^8.6.14",
    "@types/node": "^22.15.3",
    "prettier": "^3.6.2",
    "storybook": "^9.1.8",
    "storybook-addon-rslib": "^2.1.1",
    "storybook-vue3-rsbuild": "^2.1.1",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
