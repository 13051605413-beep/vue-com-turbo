{
  "name": "@yza-fe/element-plus",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./es/*.mjs": {
      "types": "./es/*.d.ts",
      "import": "./es/*.mjs"
    },

    "./es/*": {
      "types": [
        "./es/*.d.ts",
        "./es/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./lib/*": {
      "types": [
        "./lib/*.d.ts",
        "./lib/*/index.d.ts"
      ],
      "require": "./lib/*.js"
    },
    "./*": "./*"
  },

  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "style": "dist/index.css",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "rslib build && vue-tsc",
    "build:css": "rspack build",
    "watch:css": "rspack build --watch",
    "dev": "rslib build --watch && rspack build --watch"
  },
  "devDependencies": {
    "@rsbuild/plugin-less": "^1.5.0",
    "@rslib/core": "^0.16.1",
    "@rspack/cli": "^1.6.1",
    "@rspack/core": "^1.6.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "less": "^4.4.2",
    "rsbuild-plugin-unplugin-vue": "^0.1.0",
    "sass": "^1.93.3",
    "sass-loader": "^16.0.6",
    "typescript": "^5.9.3",
    "unplugin-vue": "^7.0.4",
    "vite": "^7.1.12",
    "vue": "^3.5.22",
    "vue-tsc": "^3.1.1"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "element-plus": "2.11.7"
  }
}
